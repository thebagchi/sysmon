{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:js": "react-scripts build",
    "clean:js": "rimraf ./build",
    "build:css": "tailwindcss build ./tailwind.css -o src/index.css",
    "clean:css": "rimraf ./src/style.css",
    "clean": "npm run clean:js && npm run clean:css",
    "start": "npm run build:css && react-scripts start",
    "build": "npm run clean && npm run build:css && npm run build:js"
  },
  "dependencies": {
    "@fontsource/inter": "^4.4.2",
    "@headlessui/react": "^1.2.0",
    "@heroicons/react": "^1.0.1",
    "@types/d3": "^6.7.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "d3": "^6.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.3.2",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/line-clamp": "^0.2.0",
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^10.2.6",
    "node-sass": "^6.0.0",
    "postcss": "^8.3.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.1.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
